.empty_tip{
  position: fixed;
  height: 100%;
  width: 100%;
  display:flex;
  flex-direction:column;
  align-items:center;
  justify-content: center;
}
.order_title{
  height:60px;
  font-size:14px;
  padding-top:20px;
  display:flex;
  flex-direction:row;
  justify-content: center;
  align-items:center;
}
.order_title_deskInfo{
  order:0;
}
.line{
  background: #E0E3DA;
  width: 95%;
  height: 3rpx;
  margin:8rpx auto;
}
.order_subTitle{
  font-size:14px;
  margin-left:10rpx;
  color:#ccc
}
.order_baseInfo{
  margin-top:20rpx;
}
.order_detail{
  margin-top:40rpx;
}
.order_operation{
  position:fixed;
  bottom:0;
  background-color:#f5f5f5;
  width:100%;
  text-align:center;
  height:130rpx;
}
.order_operation_btn{
  display: inline-block;
  margin:20rpx 20rpx;
  width:40%;
}
.table {
 font-size:14px;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 2rem;
}
.td {
    width:30%;
    justify-content: center;
    text-align: center;
    color:#ccc
}
.th {
  width: 30%;
  justify-content: center;
  display: flex;
  height: 2rem;
  align-items: center;
}